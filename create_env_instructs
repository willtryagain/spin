conda create -n spin python=3.8
conda install pytorch torchvision torchaudio pytorch-cuda=11.8 -c pytorch -c nvidia
 mamba  install pyg -c pyg
pip install torch_spatiotemporal==0.1.1
pip install torch-scatter -f https://data.pyg.org/whl/torch-2.1.0+cu118.html
 pip install torch-sparse -f https://data.pyg.org/whl/torch-2.1.0+cu118.html
mamba install tensorboard==2
 pip install pytorch-lightning==1.5
conda install numpy
mamba install numpy
